class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        if (nums.empty())
        {
            return 0;
        }   

        for (size_t i = nums.size() - 1; i > 0; --i)
        {
            if (nums.at(i) == nums.at(i - 1))
            {
                nums.erase(nums.begin() + i); 
            }
        }
        return nums.size();
    }
};
